<SerializableClass Type="FlightPlanXML" Version="1.2">
    <Member Name="flightplan_name">just-hover</Member>
    <Collection Name="waypoints">
        <Class Type="Waypoint" Version="1.2">
            <Member Name="type">wfm_takeoff</Member>
            <Member Name="threshold_mm"></Member>
            <Member Name="x"></Member>
            <Member Name="y"></Member>
            <Member Name="z"></Member>
        </Class>

        <!-- Middle Point -->
        <Class Type="Waypoint" Version="1.2">
            <Member Name="type">wfm_wp_stay</Member>
            <Member Name="threshold_mm">0</Member>
            <Member Name="threshold_v">0</Member>
            <Member Name="hover_pause">0</Member>
            <Member Name="x"> 0.0</Member>
            <Member Name="y">-0.9</Member>
            <Member Name="z">-2.0</Member>
            <Member Name="name">WP_mid</Member>
        </Class>

        <!-- Back to landing -->
        <Class Type="Waypoint" Version="1.2">
            <Member Name="type">wfm_yaw_reset</Member>
            <Member Name="threshold_mm"></Member>
            <Member Name="x"></Member>
            <Member Name="y"></Member>
            <Member Name="z"></Member>
        </Class> -->
        <Class Type="Waypoint" Version="1.2">
            <Member Name="type">wfm_landing</Member>
            <Member Name="threshold_mm"></Member>
            <Member Name="x"></Member>
            <Member Name="y"></Member>
            <Member Name="z"></Member>
        </Class>
    </Collection>
</SerializableClass>
