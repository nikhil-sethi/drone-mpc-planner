{
    "version": "0.2.0",
    "configurations": [

      {
        "name": "Current File",
        "type": "python",
        "args": [],
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal"
      },
      {
        "name": "jsons_to_db",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/jsons_to_db.py",
        "args": ["-i","~/jsons"],
        "console": "integratedTerminal"
      },
      {
        "name": "PATS-C",
        "type": "python",
        "request": "launch",
