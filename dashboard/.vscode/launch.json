{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Current File",
      "type": "python",
      "args": [],
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "jsons_to_db",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/jsons_to_db.py",
      "args": [
        "-i",
        "~/patsc/jsons"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "PATS-C",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "patsc",
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "1"
      },
      "args": [
        "run",
        "--no-reload"
      ],
      "jinja": true
    },
    {
      "name": "viz",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/example_viz.py",
      "args": [],
      "console": "integratedTerminal"
    },
    {
      "name": "Daily digest",
      "type": "python",
      "args": [
        "--dry-run"
      ],
      "request": "launch",
      "program": "${workspaceFolder}/daily_digest.py",
      "console": "integratedTerminal"
    },
    {
      "name": "LG",
      "type": "python",
      "args": [
        "--dry-run"
      ],
      "request": "launch",
      "program": "${workspaceFolder}/jsons_to_LG.py",
      "console": "integratedTerminal"
    },
    {
      "name": "dash_daemon",
      "type": "python",
      "args": [
        "--dry-run"
      ],
      "request": "launch",
      "program": "${workspaceFolder}/dash_daemon.py",
      "console": "integratedTerminal"
    },
  ]
}