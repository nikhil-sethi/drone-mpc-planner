server {
    server_name www.pats-c.com www.pats-c.nl pats-c.com pats-c.nl;

    location / {
        include proxy_params;
        proxy_pass http://unix:/home/pats/code/pats/dashboard/pats.sock;
    }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/pats-c.com-0001/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/pats-c.com-0001/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot




}

server {
    if ($host = pats-c.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = pats-c.nl) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = www.pats-c.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = www.pats-c.nl) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    listen 80;
    server_name www.pats-c.com www.pats-c.nl pats-c.com pats-c.nl;
    return 404; # managed by Certbot








}